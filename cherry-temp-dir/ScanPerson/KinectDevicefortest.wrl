DEF BUFFER-TEXTURE BufferTexture 
	{
		format "A8R8G8B8" ##"X8R8G8B8"
		pixelWidth 640
		pixelHeight 480
		levels 0
		parameter ["nomipmap", "animated"]
	}




 
DEF kd1 KinectDev {
	colorTexture  USE  BUFFER-TEXTURE
	coord DEF pts Coordinate {point []}
	coord4mesh DEF triPts Coordinate {point []}
}
#DEF	ds DeviceSensor	{
#	enabled	TRUE
#	device "KinectDev"
#	event USE kd
#}

DEF	ds DeviceSensor	{
	enabled	TRUE
	device "ScanPerson"
	event USE kd1
}
#DEF	 tm TimeSensor {
#loop TRUE	 enabled TRUE
#}

DEF	trans Transform	{
	translation	 0 0 -10
	children [
		DEF	btn Shape {
			geometry Rectangle {
			  size 6.4	4.8 
			}
			appearance Appearance {			
				texture	  USE  BUFFER-TEXTURE
			}
		}
		DEF	btn1 Shape {
			geometry PointSet {
				coord USE pts
			}
			appearance Appearance {material Material {emissiveColor 1 0 1}}
		}

		DEF	btn2 Shape {
			geometry  TriangleSet {
				coord USE triPts
			}
			appearance Appearance {material Material {emissiveColor 1 0 0}}

		}
		DEF	ts TouchSensor {}
	]

}


