DEF kd1 KinectDev {
	colorTexture   DEF BUFFER-TEXTURE-LEFT BufferTexture 
{
		format "X8R8G8B8" ##"X8R8G8B8"
		pixelWidth 640
		pixelHeight 480
		levels 1 
		parameter ["nomipmap", "animated"]
	}	
	colorTexture1  DEF BUFFER-TEXTURE-RIGHT BufferTexture 
	{
		format "X8R8G8B8" ##"X8R8G8B8"
		pixelWidth 640
		pixelHeight 480
		levels 1 
		parameter ["nomipmap", "animated"]#		levels 0
	}     
	coord DEF Coord Coordinate {point []}
	IndxFaceSet DEF	faceset IndexedTriangleSet{
		coord USE Coord 
		index []
	}	
}


DEF	ds DeviceSensor	{
	enabled	TRUE
	device "ScanPerson"
	event USE kd1
}
#DEF	 tm TimeSensor {
#loop TRUE	 enabled TRUE
#}

DEF	trans Transform	{
	translation	 0 0 0
	children [
		DEF	btn_left Shape {
			geometry Box {
			  size 64	48 2
			}
			appearance Appearance {			
				texture	  USE  BUFFER-TEXTURE-LEFT
				material	Material	{emissiveColor 1 1 1}
			}
		}
	]

}

DEF	trans_1 Transform	{
	translation	 70 0 0
	children [
		DEF	btn_right Shape {
			geometry Box {
			  size 64	48 2
			}
			appearance Appearance {			
				texture	  USE   BUFFER-TEXTURE-RIGHT
				material	Material	{emissiveColor 1 1 1}
			}
		}
	]

}




